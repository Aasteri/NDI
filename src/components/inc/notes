/* bootstrap 5.2 */
pt-0 /* padding-top */
pb-0 /* padding-bottom */
ps-0 /* padding-left */
pe-0 /* padding-right */
px-0 /* padding-left+right */
py-0 /* padding-top+bottom */

/* Where size is one of:
0 - setting it to 0
1 - $spacer * .25
2 - $spacer * .5
3 - $spacer
4 - $spacer * 1.5
5 - $spacer * 3 */


/* bootstrap 5.2 */
